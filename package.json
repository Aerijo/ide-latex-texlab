{
  "name": "ide-latex-texlab",
  "main": "./dist/main",
  "version": "0.3.0",
  "description": "Atom IDE package for the Texlab LaTeX language server",
  "keywords": [
    "latex",
    "ide"
  ],
  "repository": "https://github.com/Aerijo/ide-latex-texlab",
  "license": "MIT",
  "engines": {
    "atom": ">=1.34.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.9.9"
  },
  "devDependencies": {
    "@types/atom": "latest",
    "tslint": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "build": "tsc"
  },
  "configSchema": {
    "serverPath": {
      "description": "An absolute path to the the server JAR file",
      "type": "string",
      "default": ""
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "code-format.onSave": {
      "versions": {
        "0.1.0": "provideOnSaveCodeFormat"
      }
    }
  }
}
